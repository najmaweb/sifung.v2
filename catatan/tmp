	select a.unip,a.fnme,a.jbfg,a.jbtn,a.kdpel,a.kdupt,b.urjjfg,c.urjbfg from progev.pegwdata a left outer join progev.jenjang b on b.jjfg=a.jjfg left outer join progev.jabfung c on c.jbfg=a.jbfg where a.unip="197411212001121002" ;
	


SELECT pelaksana,a.kdsubutir,tgops,COUNT(urtx)frek FROM rincian01 a  LEFT OUTER JOIN header b ON b.kdsubutir=a.kdsubutir  GROUP BY a.kdsubutir,tgops  ORDER BY tgops ;



